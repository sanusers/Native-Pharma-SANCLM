<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drMainlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backround_graey"
    tools:context=".activity.homeScreen.HomeDashBoard">

    <RelativeLayout
        android:id="@+id/rl_head"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/Toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30sdp"
            android:background="@color/dark_purple"
            android:padding="0dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/back_arrow"
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="@dimen/_10sdp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left"
                    tools:ignore="ContentDescription,RtlHardcoded" />

                <TextView
                    android:id="@+id/toolbar_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/_8sdp"
                    android:layout_toEndOf="@+id/back_arrow"
                    android:fontFamily="@font/satoshi_medium"
                    android:gravity="start"
                    android:text="@string/san_media_pvt_ltd"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_8sdp"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText,RtlHardcoded,SpUsage,TextSizeCheck" />

                <ImageView
                    android:id="@+id/img_location"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_toStartOf="@id/img_notofication"
                    android:src="@drawable/location_img"
                    tools:ignore="ContentDescription" />

                <ImageView
                    android:visibility="gone"
                    android:id="@+id/img_notofication"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_toStartOf="@id/img_sync"
                    android:src="@drawable/chat_img"
                    tools:ignore="ContentDescription" />

                <ImageView
                    android:id="@+id/img_sync"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_toStartOf="@id/img_account"
                    android:src="@drawable/sync_img"
                    tools:ignore="ContentDescription" />


                <ImageView
                    android:id="@+id/img_account"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_marginBottom="@dimen/_2sdp"
                    android:src="@drawable/accout_img"
                    tools:ignore="ContentDescription" />

                <RelativeLayout
                    android:id="@+id/img_account1"
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="@dimen/_10sdp"
                    android:layout_below="@+id/img_account"
                    android:background="@drawable/triangle_view" />

            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>


        <androidx.drawerlayout.widget.DrawerLayout
            android:id="@+id/my_drawer_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/Toolbar"

            tools:context=".activity.DashBoard.HomeDashBoard">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:layout_weight="1.8"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:id="@+id/rl_date_layoout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginStart="@dimen/_4sdp"
                        android:layout_marginTop="@dimen/_3sdp"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:background="@drawable/custom_backround_layout"
                        android:backgroundTint="@color/white"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/caleder_img"
                            android:layout_width="@dimen/_9sdp"
                            android:layout_height="@dimen/_9sdp"
                            android:layout_alignParentStart="true"
                            android:layout_centerInParent="true"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:src="@drawable/calendar_img"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/text_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="center"
                            android:hint="@string/select_date"
                            android:padding="@dimen/_5sdp"
                            android:text=""
                            android:textColor="@color/text_dark"
                            android:textColorHint="@color/text_dark"
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="bold"
                            tools:ignore="RelativeOverlap,SpUsage,TextSizeCheck" />

                        <ImageView
                            android:id="@+id/img_double_vecer"
                            android:layout_width="@dimen/_9sdp"
                            android:layout_height="@dimen/_9sdp"
                            android:layout_alignParentEnd="true"
                            android:layout_centerInParent="true"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:src="@drawable/arrow_bot_top_img"
                            tools:ignore="ContentDescription" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <com.google.android.material.tabs.TabLayout
                                android:id="@+id/tab_layout"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_25sdp"
                                android:layout_marginStart="@dimen/_4sdp"
                                android:layout_marginEnd="@dimen/_4sdp"
                                android:background="@color/backround_graey"
                                app:tabGravity="center"
                                app:tabIndicatorColor="@color/text_dark"
                                app:tabIndicatorGravity="bottom"
                                app:tabIndicatorHeight="@dimen/_1sdp"
                                app:tabMode="fixed"
                                app:tabSelectedTextColor="@color/text_dark"
                                app:tabTextAppearance="@style/tab_text"
                                app:tabTextColor="@color/text_dark_65" />

                            <View
                                android:id="@+id/view_dummy"
                                android:layout_width="match_parent"
                                android:layout_height="0.5dp"
                                android:background="#9e9ea5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/tablelayout"
                                app:layout_constraintVertical_bias="0.0" />

                            <androidx.viewpager.widget.ViewPager
                                android:id="@+id/view_pager"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_margin="@dimen/_4sdp"
                                android:layout_marginStart="@dimen/_4sdp"
                                android:layout_marginTop="@dimen/_6sdp"
                                android:layout_marginEnd="@dimen/_4sdp"
                                android:layout_weight="1"
                                android:background="@drawable/backround_text"
                                android:backgroundTint="@color/white"
                                tools:ignore="NestedWeights" />

                            <!--  <include
                                  android:id="@+id/tab_layout"
                                  layout="@layout/customize_tablayout"
                                  android:layout_width="match_parent"
                                  android:layout_height="wrap_content"
                                  android:layout_marginStart="@dimen/_4sdp"
                                  android:layout_marginEnd="@dimen/_4sdp" />

                              <androidx.viewpager2.widget.ViewPager2
                                  android:id="@+id/view_pager"
                                  android:layout_width="match_parent"
                                  android:layout_height="0dp"
                                  android:layout_margin="@dimen/_4sdp"
                                  android:layout_marginStart="@dimen/_4sdp"
                                  android:layout_marginTop="@dimen/_6sdp"
                                  android:layout_marginEnd="@dimen/_4sdp"
                                  android:layout_weight="1"
                                  android:background="@drawable/backround_text"
                                  android:backgroundTint="@color/white"
                                  tools:ignore="NestedWeights" />-->

                            <include
                                android:id="@+id/view_calerder_layout"
                                layout="@layout/call_status_calender_layout"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/_4sdp"
                                android:layout_marginTop="@dimen/_6sdp"
                                android:layout_marginEnd="@dimen/_4sdp"
                                android:visibility="gone" />
                        </LinearLayout>

                        <ProgressBar
                            android:id="@+id/view_pager_progress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:indeterminateTint="@color/dark_purple"
                            android:visibility="visible" />

                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <saneforce.sanzen.activity.homeScreen.adapters.CustomViewPager
                        android:id="@+id/view_pager1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_2sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:layout_weight="1"
                        android:backgroundTint="@color/white"
                        tools:ignore="NestedWeights,SpeakableTextPresentCheck" />

                    <RelativeLayout
                        android:id="@+id/rl_quick_link"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:layout_marginBottom="@dimen/_2sdp"
                        android:layout_weight="5">

                        <TextView
                            android:id="@+id/txt_quik_link"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/satoshi_medium"
                            android:text="Quick Link"
                            android:textColor="@color/text_dark"
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="bold"
                            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

                        <HorizontalScrollView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_below="@+id/txt_quik_link">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginTop="@dimen/_4sdp"
                                android:layout_marginEnd="@dimen/_2sdp"
                                android:orientation="horizontal"
                                android:radius="@dimen/_6sdp">

                                <LinearLayout
                                    android:id="@+id/ll_presentation"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:layout_marginEnd="@dimen/_2sdp"
                                    android:layout_marginBottom="@dimen/_2sdp"
                                    android:background="@drawable/backround_text"
                                    android:backgroundTint="#c6eae1"
                                    android:orientation="horizontal"
                                    android:padding="@dimen/_2sdp">

                                    <ImageView
                                        android:layout_width="@dimen/_9sdp"
                                        android:layout_height="@dimen/_9sdp"
                                        android:layout_gravity="center|start"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:contentDescription="TODO"
                                        android:src="@drawable/image_img"
                                        tools:ignore="ContentDescription,HardcodedText,ImageContrastCheck" />


                                    <TextView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="start|center"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/satoshi_medium"
                                        android:gravity="start"
                                        android:text="@string/presentation"
                                        android:textColor="@color/black_45"
                                        android:textSize="@dimen/_7sdp"
                                        tools:ignore="SpUsage,TextSizeCheck" />

                                    <ImageView
                                        android:layout_width="@dimen/_8sdp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center|end"
                                        android:layout_marginEnd="@dimen/_5sdp"
                                        android:src="@drawable/arrow_right_"
                                        tools:ignore="ContentDescription" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ll_slide"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_margin="@dimen/_2sdp"
                                    android:background="@drawable/backround_text"
                                    android:backgroundTint="#d3e4ef"
                                    android:orientation="horizontal"
                                    android:padding="@dimen/_2sdp">

                                    <ImageView
                                        android:layout_width="@dimen/_9sdp"
                                        android:layout_height="@dimen/_9sdp"
                                        android:layout_gravity="center|start"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:src="@drawable/eye_img"
                                        tools:ignore="ContentDescription,ImageContrastCheck" />


                                    <TextView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="start|center"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/satoshi_medium"
                                        android:gravity="start"
                                        android:text="@string/preview"
                                        android:textColor="@color/black_45"
                                        android:textSize="@dimen/_7sdp"
                                        tools:ignore="SpUsage,TextSizeCheck">

                                    </TextView>

                                    <ImageView
                                        android:layout_width="@dimen/_8sdp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center|end"
                                        android:layout_marginEnd="@dimen/_5sdp"
                                        android:src="@drawable/arrow_right_"
                                        tools:ignore="ContentDescription">

                                    </ImageView>
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ll_report"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_margin="@dimen/_2sdp"
                                    android:background="@drawable/backround_text"
                                    android:backgroundTint="#f2d7db"
                                    android:orientation="horizontal"
                                    android:padding="@dimen/_2sdp">

                                    <ImageView
                                        android:layout_width="@dimen/_9sdp"
                                        android:layout_height="@dimen/_9sdp"
                                        android:layout_gravity="center|start"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:src="@drawable/file_chart_img"
                                        tools:ignore="ContentDescription,ImageContrastCheck" />

                                    <TextView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center|start"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/satoshi_medium"
                                        android:gravity="start"
                                        android:text="@string/report"
                                        android:textColor="@color/black_45"
                                        android:textSize="@dimen/_7sdp"
                                        tools:ignore="SpUsage,TextSizeCheck">

                                    </TextView>

                                    <ImageView
                                        android:layout_width="@dimen/_8sdp"
                                        android:layout_height="@dimen/_8sdp"
                                        android:layout_gravity="center|end"
                                        android:layout_marginEnd="@dimen/_5sdp"
                                        android:src="@drawable/arrow_right_"
                                        tools:ignore="ContentDescription">

                                    </ImageView>

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ll_analys"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:layout_marginEnd="@dimen/_2sdp"
                                    android:layout_marginBottom="@dimen/_2sdp"
                                    android:visibility="gone"
                                    android:background="@drawable/backround_text"
                                    android:backgroundTint="#26FFEB3B"
                                    android:orientation="horizontal"
                                    android:padding="@dimen/_2sdp">

                                    <ImageView
                                        android:layout_width="@dimen/_9sdp"
                                        android:layout_height="@dimen/_9sdp"
                                        android:layout_gravity="center|start"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:src="@drawable/image_img"
                                        tools:ignore="ContentDescription,ImageContrastCheck" />

                                    <TextView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center|start"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/satoshi_medium"
                                        android:gravity="start"
                                        android:text="@string/analysis"
                                        android:textColor="@color/black_45"
                                        android:textSize="@dimen/_7sdp"
                                        tools:ignore="SpUsage,TextSizeCheck" />

                                    <ImageView
                                        android:layout_width="@dimen/_8sdp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center|end"
                                        android:layout_marginEnd="@dimen/_5sdp"
                                        android:src="@drawable/arrow_right_"
                                        tools:ignore="ContentDescription">

                                    </ImageView>

                                </LinearLayout>

                            </LinearLayout>
                        </HorizontalScrollView>
                    </RelativeLayout>
                </LinearLayout>

            </LinearLayout>


            <com.google.android.material.navigation.NavigationView
                android:id="@+id/nav_view"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="start"
                android:background="@drawable/background_navicationview"
                android:paddingStart="@dimen/_8sdp"
                android:paddingEnd="@dimen/_8sdp"
                app:itemBackground="@drawable/drawer_item_bg"
                app:itemIconSize="@dimen/_10sdp"
                app:menu="@menu/navicationdrawermenuitem"
                app:itemTextAppearance="@style/CustomNavDrawerItemTextStyle"
                app:itemTextColor="@color/black_45">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:background="#ffffff">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3sdp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:visibility="visible">

                        <ImageView
                            android:id="@+id/sequential_dot"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:background="@drawable/round_back_brown"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="center"
                            android:visibility="gone"
                            android:src="@drawable/baseline_repeat_24"
                            android:padding="3dp"/>

                        <TextView
                            android:id="@+id/tvLdot"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:layout_marginRight="@dimen/_2sdp"
                            android:background="@drawable/round_back_yellow"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="center"
                            android:text="L"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_6sdp"
                            android:textStyle="bold"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tvDdot"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:layout_marginRight="@dimen/_2sdp"
                            android:background="@drawable/round_back_green"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="center"
                            android:padding="0dp"
                            android:text="D"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_6sdp"
                            android:textStyle="bold"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tvCdot"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:layout_marginRight="@dimen/_2sdp"
                            android:background="@drawable/round_back_blue"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="center"
                            android:padding="0dp"
                            android:text="C"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_6sdp"
                            android:textStyle="bold"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tvSdot"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:layout_marginRight="@dimen/_2sdp"
                            android:background="@drawable/round_back_pink"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="center"
                            android:padding="0dp"
                            android:text="S"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_6sdp"
                            android:textStyle="bold"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tvUdot"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:background="@drawable/round_back_gray"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="center"
                            android:padding="0dp"
                            android:text="U"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_6sdp"
                            android:textStyle="bold"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tvHdot"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:background="@drawable/round_back_violet"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="center"
                            android:padding="0dp"
                            android:text="H"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/_6sdp"
                            android:textStyle="bold"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/app_vers"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/satoshi_medium"
                            android:gravity="end"
                            android:padding="@dimen/_2sdp"
                            android:text="@string/app_version"
                            android:textSize="@dimen/_7sdp"
                            android:textStyle="bold" />

                    </LinearLayout>
                </RelativeLayout>
            </com.google.android.material.navigation.NavigationView>
        </androidx.drawerlayout.widget.DrawerLayout>

    </RelativeLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view1"
        android:layout_width="@dimen/_180sdp"
        android:layout_height="match_parent"
        android:layout_gravity="end"
        android:background="@drawable/backround_text"
        android:backgroundTint="@color/white"
        android:foregroundGravity="center">

        <include
            android:id="@+id/llNav"
            layout="@layout/list_item_worktye"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/_2sdp"
            tools:ignore="DuplicateIncludedIds" />

    </com.google.android.material.navigation.NavigationView>


</androidx.drawerlayout.widget.DrawerLayout>