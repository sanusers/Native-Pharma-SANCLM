<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/text_call_analysis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/_3sdp"
        android:fontFamily="@font/satoshi_medium"
        android:text="Sales Analysis"
        android:textColor="@color/text_dark"
        android:textSize="@dimen/_8sdp"
        android:textStyle="bold" />


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_2sdp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="@dimen/_20sdp"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_1sdp"
            android:src="@drawable/tab_litte_line" />

        <ImageView
            android:layout_width="@dimen/_20sdp"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_1sdp"
            android:src="@drawable/tab_litte_line" />

        <ImageView
            android:layout_width="@dimen/_20sdp"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_1sdp"
            android:src="@drawable/tab_dark_line" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/text_call_analysis"
        android:layout_marginTop="@dimen/_10sdp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"></LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_2sdp"
            android:layout_weight="1.8"
            android:background="@drawable/backround_lite_grey_border">

            <TextView
                android:id="@+id/text_month"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="1dp"
                android:layout_marginTop="1dp"

                android:layout_marginBottom="1dp"
                android:layout_weight="1"
                android:background="@drawable/custom_background_black"
                android:fontFamily="@font/satoshi_medium"
                android:gravity="center"
                android:text="Monthly"
                android:textColor="@color/white"
                android:textSize="@dimen/_6sdp"></TextView>

            <TextView
                android:id="@+id/text_Quaterly"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"
                android:layout_weight="1"
                android:background="@drawable/cutom_backround_grey"
                android:fontFamily="@font/satoshi_medium"
                android:gravity="center"
                android:text="Quarterly"
                android:textColor="@color/txt_lite_black"
                android:textSize="@dimen/_6sdp"></TextView>

            <TextView
                android:id="@+id/text_yearly"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginBottom="1dp"
                android:layout_weight="1"
                android:background="@drawable/cutom_backround_grey"
                android:fontFamily="@font/satoshi_medium"
                android:gravity="center"
                android:text="Yearly"
                android:textColor="@color/txt_lite_black"
                android:textSize="@dimen/_6sdp"></TextView>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/layout_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/layout2"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.6">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_2sdp"
                android:layout_weight="1"
                android:background="@drawable/custom_backround_layout"
                android:elevation="0dp"
                app:cardCornerRadius="@dimen/_2sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center">


                    <TextView
                        android:id="@+id/text_Primary"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="Primary Sales"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_6sdp"
                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_Primary_value"
                        android:layout_width="wrap_content"

                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_Primary"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="106.56L"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_12sdp"
                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_Primary_target"
                        android:layout_width="wrap_content"

                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_Primary_value"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_regular"
                        android:text="Target 282.2L"
                        android:textColor="#a9a8af"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_primary_growth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/text_Primary"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:layout_toRightOf="@+id/text_Primary_value"
                        android:background="@drawable/custom_backround_layout"

                        android:backgroundTint="#d4f8ef"
                        android:fontFamily="@font/satoshi_regular"
                        android:padding="@dimen/_2sdp"
                        android:text="39% Growth"
                        android:textColor="#00c689"
                        android:textSize="@dimen/_4sdp"
                        android:textStyle="bold">

                    </TextView>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_2sdp"
                android:layout_weight="1"
                android:background="@drawable/custom_backround_layout"
                android:elevation="0dp"
                app:cardCornerRadius="@dimen/_2sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center">
                    <TextView
                        android:id="@+id/text_secondary"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="Secondary Sales"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>

                    <TextView
                        android:id="@+id/text_seondary_growth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/text_secondary"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:layout_toRightOf="@+id/text_secondary_value"
                        android:background="@drawable/custom_backround_layout"
                        android:backgroundTint="#e1f2fd"
                        android:padding="@dimen/_2sdp"
                        android:text="39% Growth"
                        android:textColor="#41a1ef"
                        android:textSize="@dimen/_4sdp"
                        android:textStyle="bold">

                    </TextView>

                    <TextView
                        android:id="@+id/text_secondary_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_secondary"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="106.56L"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_12sdp"
                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_secondary_target"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_secondary_value"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_regular"
                        android:text="Target 282.2L"
                        android:textColor="#a9a8af"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>


                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_2sdp"
                android:layout_weight="1"
                android:background="@drawable/custom_backround_layout"
                android:elevation="0dp"
                app:cardCornerRadius="@dimen/_2sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center">


                    <TextView
                        android:id="@+id/text_RCPA1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="RCPA"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>

                    <TextView
                        android:id="@+id/text_RCPA_growth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/text_RCPA1"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:layout_toRightOf="@+id/text_RCPA1_value"
                        android:background="@drawable/custom_backround_layout"
                        android:backgroundTint="#ffe5e9"
                        android:padding="@dimen/_2sdp"
                        android:text="39% Growth"
                        android:textColor="#fe5876"
                        android:textSize="@dimen/_4sdp"
                        android:textStyle="bold">

                    </TextView>

                    <TextView
                        android:id="@+id/text_RCPA1_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/text_RCPA1"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="106.56L"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_12sdp"
                        android:textStyle="bold"

                        >

                    </TextView>


                    <TextView
                        android:id="@+id/text_RCPA1_target"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_RCPA1_value"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_regular"
                        android:text="Target 282.2L"
                        android:textColor="#a9a8af"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>


                </RelativeLayout>
            </androidx.cardview.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_2sdp"
                android:layout_weight="1"
                android:background="@drawable/custom_backround_layout"
                android:elevation="0dp"
                app:cardCornerRadius="@dimen/_2sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@drawable/custom_backround_layout"
                    android:elevation="5dp">

                    <TextView
                        android:id="@+id/text_PCPM"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="PCPM"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>

                    <TextView
                        android:id="@+id/text_PCPM_growth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/text_PCPM"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:layout_toRightOf="@+id/text_PCPM_value"
                        android:background="@drawable/custom_backround_layout"
                        android:backgroundTint="#fef6df"
                        android:padding="@dimen/_2sdp"
                        android:text="39% Growth"
                        android:textColor="#feda86"
                        android:textSize="@dimen/_4sdp"
                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_PCPM_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_PCPM"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="106.56L"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_12sdp"
                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_PCPM_target"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_PCPM_value"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_regular"
                        android:text="Target 282.2L"
                        android:textColor="#a9a8af"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>


                    <RelativeLayout
                        android:id="@+id/dcr_call_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/text_PCPM_target"
                        android:gravity="right">

                        <ProgressBar
                            android:id="@+id/progress_bar_doc"
                            style="@style/CircularDeterminateProgressBar"
                            android:layout_width="@dimen/_35sdp"
                            android:layout_height="@dimen/_35sdp"
                            android:layout_centerInParent="true"
                            android:layout_marginStart="@dimen/_4sdp"
                            android:layout_marginTop="@dimen/_4sdp"
                            android:layout_marginEnd="@dimen/_4sdp"
                            android:layout_marginBottom="@dimen/_4sdp"
                            android:indeterminate="false"
                            android:progress="60"
                            android:progressDrawable="@drawable/progressbar_circular"
                            android:rotation="-90" />

                        <TextView
                            android:id="@+id/doc_progress_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="25%"
                            android:textColor="#bab9bf" />
                    </RelativeLayout>

                </RelativeLayout>
            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_2sdp"
                android:layout_weight="1"
                android:background="@drawable/custom_backround_layout"
                android:elevation="0dp"
                app:cardCornerRadius="@dimen/_2sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@drawable/custom_backround_layout">


                    <TextView
                        android:id="@+id/text_BDL"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="BDL"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_7sdp"

                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_BDL_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_BDL"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="106.56L"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_12sdp"
                        android:textStyle="bold">

                    </TextView>

                    <TextView
                        android:id="@+id/text_BDL_target"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_BDL_value"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_regular"
                        android:text="Target 282.2L"
                        android:textColor="#a9a8af"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold"

                        >

                    </TextView>


                    <TextView
                        android:id="@+id/text_BDL_target1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_BDL_target"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="Target"
                        android:textColor="#85858f"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold"


                        >

                    </TextView>


                    <TextView
                        android:id="@+id/text_BDL_actual"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_BDL_target"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="Actual"
                        android:textColor="#00c689"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold"

                        >

                    </TextView>

                    <TextView
                        android:id="@+id/text_BDL_target_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_BDL_target1"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="282.2L"
                        android:textColor="#85858f"

                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_tareget_value_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_BDL_target1"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="26.75L"
                        android:textColor="#00c689"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>

                </RelativeLayout>
            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_2sdp"
                android:layout_weight="1"
                android:background="@drawable/custom_backround_layout"
                android:elevation="0dp"
                app:cardCornerRadius="@dimen/_2sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:gravity="left">


                    <TextView
                        android:id="@+id/text_RCPA"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="RCPA"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold">

                    </TextView>


                    <TextView
                        android:id="@+id/text_RCPA_value"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_RCPA"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_7sdp"
                        android:fontFamily="@font/satoshi_medium"
                        android:text="106.56L"
                        android:textColor="@color/text_dark"
                        android:textSize="@dimen/_12sdp"
                        android:textStyle="bold" />


                    <TextView
                        android:id="@+id/text_RCPA_target"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/text_RCPA_value"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_7sdp"
                        android:fontFamily="@font/satoshi_regular"
                        android:text="Target 282.2L"

                        android:textColor="#a9a8af"
                        android:textSize="@dimen/_7sdp"
                        android:textStyle="bold" />


                </RelativeLayout>
            </androidx.cardview.widget.CardView>

        </LinearLayout>

    </LinearLayout>


</RelativeLayout>