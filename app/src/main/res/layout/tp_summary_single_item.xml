<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/_3sdp"
        android:layout_marginStart="@dimen/_6sdp"
        android:layout_marginEnd="@dimen/_6sdp"
        android:layout_marginTop="@dimen/_3sdp"
        android:layout_marginBottom="@dimen/_3sdp"
        android:background="@drawable/edittext_stroke"
        android:backgroundTint="#eeeeee"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Date"
                android:textSize="@dimen/_5sdp"
                android:textColor="@color/text_dark"
                android:fontFamily="@font/satoshi_variable"
                android:layout_alignParentStart="true"
                android:gravity="center"
                tools:ignore="SpUsage"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:letterSpacing="0.2"
                android:text="..."
                android:textColor="@color/text_dark"
                android:textSize="@dimen/_6sdp"
                tools:ignore="SpUsage" />

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/workType1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_3sdp">

            <TextView
                android:layout_weight="1"
                android:id="@+id/workTypeLabel"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Field Work3"
                android:textSize="@dimen/_5sdp"
                android:textColor="@color/text_dark_65"
                android:fontFamily="@font/satoshi_variable"
                android:singleLine="true"
                android:paddingEnd="@dimen/_5sdp"
                tools:ignore="SpUsage"/>

            <View
                android:layout_width="1dp"
                android:layout_height="@dimen/_6sdp"
                android:background="#4D282A3C"
                android:layout_gravity="center"/>

            <TextView
                android:layout_weight="1"
                android:id="@+id/hqName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="HQ - Shiva kumar 3"
                android:textColor="@color/text_dark_65"
                android:textSize="@dimen/_5sdp"
                android:fontFamily="@font/satoshi_variable"
                android:ellipsize="end"
                android:singleLine="true"
                android:paddingStart="@dimen/_5sdp"
                tools:ignore="SpUsage"/>

        </LinearLayout>

        <LinearLayout
