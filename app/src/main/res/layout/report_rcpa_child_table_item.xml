<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/_5sdp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="@dimen/_1sdp"
            android:orientation="horizontal"
            android:weightSum="6">

            <TextView
                android:id="@+id/productname"
                android:fontFamily="@font/satoshi_medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@color/dark_purple"
                android:textSize="@dimen/_6sdp"
                android:layout_weight="1"
                android:text="ProductName" />

            <TextView
                android:id="@+id/productqty"
                android:fontFamily="@font/satoshi_medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@color/dark_purple"
                android:textSize="@dimen/_6sdp"
                android:layout_weight="0.5"
                android:text="Qty" />
            <TextView
                android:id="@+id/competitorname"
                android:layout_width="0dp"
                android:textColor="@color/dark_purple"
                android:textSize="@dimen/_6sdp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/satoshi_medium"
                android:layout_weight="1"
                android:text="@string/chemist_name" />
            <TextView
                android:id="@+id/chemistName"
                android:layout_width="0dp"
                android:textColor="@color/dark_purple"
                android:textSize="@dimen/_6sdp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/satoshi_medium"
                android:layout_weight="1.5"
                android:text="Competitor Name" />

            <TextView
                android:id="@+id/competitorproductname"
                android:layout_width="0dp"
                android:fontFamily="@font/satoshi_medium"
                android:textColor="@color/dark_purple"
                android:layout_height="wrap_content"
                android:layout_weight="1.5"
                android:textSize="@dimen/_6sdp"
                android:text="Competitor Productname" />

            <TextView
                android:id="@+id/competitorproductnameqty"
                android:layout_width="0dp"
                android:textSize="@dimen/_6sdp"
                android:fontFamily="@font/satoshi_medium"
                android:textColor="@color/dark_purple"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:text="Qty" />

        </LinearLayout>
